            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^7.6.0 || ^8.7.0",
        "rkw/rkw-basics": ">=7.6.10",
        "rkw/rkw-mailer": ">=7.6.10",
        "rkw/rkw-registration": ">=7.6.10",
		"rkw/rkw-projects": ">=7.6.10"
